{
    "name": "drewlabs/curl-client",
    "description": "Provides a PHP class around PHP curl object and Psr18 client based on the curl library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\Curl\\": "src/"
        }
    },
    "keywords": [
        "curl",
        "http"
    ],
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "ext-curl": "*"
    },
    "require-dev": {
        "donatj/mock-webserver": "^2.5",
        "phpunit/phpunit": ">=8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
